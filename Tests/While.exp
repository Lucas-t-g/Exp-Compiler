# results in 1000000 -> 50000

total = 0
x = 0
while x < 100 {
    y = 1
    while y < 100 {
        total = total + y
        y = y + 1
        if y == 50{
            break
        }
    }
    z = 100
    while z > 0 {
        total = total + z
        z = z - 1
        if z == 50 {
            break
        }
    }
    x = x + 1
    if x == 10 {
        break
    }
}
print(total)
